{
    "workbench.iconTheme": "material-icon-theme",
    "editor.inlineSuggest.enabled": true,
    "editor.fontFamily": "Consolas, 'MesloLGS NF', monospace",
    "vim.useSystemClipboard": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindings": [
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "c"
            ],
            "after": [
                "\"_c"
            ]
        },
        {
            "before": [
                "C"
            ],
            "after": [
                "\"_C"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "\"_x"
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "\"_s"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "U"
            ],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "N"
            ],
            "after": [
                "N",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "*"
            ],
            "after": [
                "*",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "#"
            ],
            "after": [
                "#",
                "z",
                "z"
            ]
        }
    ],
    "vim.visualModeKeyBindings": [
        {
            "before": [
                "H"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "L"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "c"
            ],
            "after": [
                "\"_c"
            ]
        },
        {
            "before": [
                "x"
            ],
            "after": [
                "\"_x"
            ]
        },
        {
            "before": [
                "s"
            ],
            "after": [
                "\"_s"
            ]
        },
        {
            "before": [
                "C"
            ],
            "after": [
                "\"_C"
            ]
        },
        {
            "before": [
                "u"
            ],
            "commands": [
                {
                    "command": "undo"
                }
            ]
        },
        {
            "before": [
                "U"
            ],
            "commands": [
                {
                    "command": "redo"
                }
            ]
        },
        {
            "before": [
                "n"
            ],
            "after": [
                "n",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "N"
            ],
            "after": [
                "N",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "*"
            ],
            "after": [
                "*",
                "z",
                "z"
            ]
        },
        {
            "before": [
                "#"
            ],
            "after": [
                "#",
                "z",
                "z"
            ]
        }
    ],
    "git.suggestSmartCommit": false,
    "git.ignoreRebaseWarning": true,
    "[novel]": {},
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter",
        "editor.codeActionsOnSave": {
            "source.organizeImports": "explicit"
        },
    },
    "isort.args": [
        "--profile",
        "black"
    ],
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "cursor.cpp.disabledLanguages": [
        "plaintext",
        "scminput",
        "novel"
    ],
    "workbench.editor.enablePreview": false,
    "markdown-preview-enhanced.enablePreviewZenMode": false,
    "markdown-preview-enhanced.codeBlockTheme": "github-dark.css",
    "markdown-preview-enhanced.revealjsTheme": "black.css",
    "markdown-preview-enhanced.previewTheme": "github-dark.css",
    "extensions.experimental.affinity": {
        "asvetliakov.vscode-neovim": 1
    },
    "workbench.editor.empty.hint": "hidden",
    "workbench.tree.indent": 21,
    "workbench.editor.pinnedTabsOnSeparateRow": true,
    "vim.statusBarColorControl": true,
    "workbench.colorCustomizations": {
        "statusBar.background": "#FFE119",
        "statusBar.foreground": "#000000",
        "statusBarItem.remoteBackground": "#E6194B",
        "statusBarItem.remoteForeground": "#FFFFFF",
        "statusBar.noFolderBackground": "#FFE119",
        "statusBar.debuggingBackground": "#FFE119",
        "statusBar.debuggingForeground": "#000000"
    },
    "vim.statusBarColors.normal": [
        "#3CB44B",
        "#FFFFFF"
    ],
    "vim.statusBarColors.insert": [
        "#E6194B",
        "#FFFFFF"
    ],
    "vim.statusBarColors.visual": [
        "#FFE119",
        "#000000"
    ],
    "vim.statusBarColors.visualline": [
        "#FFE119",
        "#000000"
    ],
    "vim.statusBarColors.visualblock": [
        "#FFE119",
        "#000000"
    ],
    "vim.statusBarColors.replace": [
        "#F97316",
        "#FFFFFF"
    ],
    "vim.statusBarColors.commandlineinprogress": [
        "#1793D1",
        "#FFFFFF"
    ],
    "vim.statusBarColors.searchinprogress": [
        "#1793D1",
        "#FFFFFF"
    ]
}
